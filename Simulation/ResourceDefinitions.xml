    </ResourceLocation>
    <ResourceLocation Name="ActionPoint" LocationPath="EmpireTypeMajor/Garrison/ClassUnit">
      <StockPropertyName>ActionPoint</StockPropertyName>
    </ResourceLocation>
  </ResourceDefinition>
  
