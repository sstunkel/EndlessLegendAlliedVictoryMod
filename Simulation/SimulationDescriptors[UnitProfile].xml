<?xml version="1.0" encoding="utf-8" ?>
<Datatable xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

  <SimulationDescriptor Name="UnitHero" Type="UnitHero">
    <SimulationPropertyDescriptors>
      <SimulationProportionalPropertyDescriptor Name="AssignmentCooldown" StartingRatio="0" Maximum="$(MaximumAssignmentCooldown)"/>
      <SimulationPropertyDescriptor Name="MaximumAssignmentCooldown"  BaseValue="5"/>
      <SimulationPropertyDescriptor Name="HeroUpkeep"                 BaseValue="0" MinValue="0"/>
      
      <!-- Hero upkeep related property -->
      <SimulationPropertyDescriptor Name="HeroCount" BaseValue="1"/>

      <!-- Injured related properties-->
      <SimulationProportionalPropertyDescriptor Name="CurrentInjuredValue" StartingRatio="0" Maximum="$(MaximumInjuredValue)"/>
      <SimulationPropertyDescriptor Name="MaximumInjuredValue" BaseValue="100"/>
      <SimulationPropertyDescriptor Name="InjuredRecoveryPerTurn" BaseValue="5"/>
      <!-- healing an injusred hero : the injured percentage is multiplied by this next factor to set the dust cost-->
      <SimulationPropertyDescriptor Name="InjuredValueToEmpireMoneyConversion" BaseValue="0"/>

    </SimulationPropertyDescriptors>
    
    <SimulationModifierDescriptors>
      <SimulationModifierDescriptor             TargetProperty="UnitSlotCount"  Operation="Force"           Value="0"/>
      <BinarySimulationModifierDescriptor       TargetProperty="HeroUpkeep"     Operation="Addition"        Left="$(Level)" BinaryOperation="Subtraction"     Right="1" Priority="0"/>
      <BinarySimulationModifierDescriptor       TargetProperty="HeroUpkeep"     Operation="Multiplication"  Left="$(Level)" BinaryOperation="Division"        Right="4" Priority="1"/>
      <BinarySimulationModifierDescriptor       TargetProperty="HeroUpkeep"     Operation="Addition"        Left="2"        BinaryOperation="Multiplication"  Right="$(LevelDisplayed)" Priority="2"/>
      <SimulationModifierDescriptor             TargetProperty="HeroUpkeep"     Operation="Addition"        Value="$(SkillPointsSpent)"/>
      
      <!-- don't use the sum system to avoid to create properties in army and city class-->
      <SimulationModifierDescriptor TargetProperty="HeroUpkeep"     Operation="Addition"  Value="$(HeroUpkeep)"         Path="../ClassEmpire"/>
      
      <SimulationModifierDescriptor       TargetProperty="InjuredValueToEmpireMoneyConversion" Operation="Addition"       Value="0.5"/>
      <BinarySimulationModifierDescriptor TargetProperty="InjuredValueToEmpireMoneyConversion" Operation="Multiplication" Left="1" BinaryOperation="Addition" Right="$(Level)" />
    </SimulationModifierDescriptors>
  </SimulationDescriptor>



