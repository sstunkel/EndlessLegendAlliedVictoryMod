  <AIPersonalityDefinition Name="PersonalityMajorFactionGameDifficultyNewbie">
    <Multipliers>
      <!-- AI Registry -->
      <AI>
        <MajorEmpire>
          <NumberOfTurnInBesiege>1.5</NumberOfTurnInBesiege>
        </MajorEmpire>
      </AI>

      <!-- AgentDefinition Database -->
      <AgentDefinition>
        
        <!-- Economic Agents -->
        
        <TechnologyReferenceTurnCountAgent>
          <IdealTechnologyUnlockPeriod>1.5</IdealTechnologyUnlockPeriod>
          <MaximumPeriodGap>1.5</MaximumPeriodGap>
        </TechnologyReferenceTurnCountAgent>

        <DiplomacyNeedPrestigeRatioAgent>
          <NumberOfTurnToReachObjective>1.5</NumberOfTurnToReachObjective>
        </DiplomacyNeedPrestigeRatioAgent>

        <EmpirePlanNeedPrestigeRatioAgent> <!-- The AI will want to stock X% of the wanted empire plan cost. -->
          <WantedEmpirePlanCostRatio>1.5</WantedEmpirePlanCostRatio>
        </EmpirePlanNeedPrestigeRatioAgent>

        <IndustryReferenceTurnCountAgent>
          <MaximumTurnCount>1.5</MaximumTurnCount>
        </IndustryReferenceTurnCountAgent>

        <MoneyReferenceRatioAgent>
          <MoneyStockToMaintainUpkeepTurnCount>1.5</MoneyStockToMaintainUpkeepTurnCount>  <!-- We want a bank account at least greater than X turns upkeep. -->
          <MoneyIncomeGrowthPercent>1.5</MoneyIncomeGrowthPercent>                        <!-- We want an income greater than X% of the bank account (positive snowball effect). -->
        </MoneyReferenceRatioAgent>

        <PopulationBuyoutReferenceRatioAgent>
          <MoneyIncomeBuyoutPercent>1.5</MoneyIncomeBuyoutPercent>
        </PopulationBuyoutReferenceRatioAgent>

        <PopulationReferenceTurnCountAgent>
          <RangeRatio>1.5</RangeRatio>          <!-- Define the maximum criticy point. MaximumNumberOfTurn = RangeRatio *  IdealNumberOfTurnToReachNextPopulation -->
        </PopulationReferenceTurnCountAgent>

        <!-- Diplomatic Agents -->

        <WarTermAgent>
          <LivingspaceTenseMultiplier>1</LivingspaceTenseMultiplier>
          <CommonFrontierMultiplier>1</CommonFrontierMultiplier>
          <AggressiveColonizationMultiplier>1</AggressiveColonizationMultiplier>
          <MilitaryPowerMultiplier>1</MilitaryPowerMultiplier>
          <RegionCountMultiplier>1</RegionCountMultiplier>
          <OtherEmpireLeadScoreMultiplier>1</OtherEmpireLeadScoreMultiplier>
          <UnitsInTerritoryMultiplier>1</UnitsInTerritoryMultiplier>
          <AggressionInNeutralRegionDuringColdWarMultiplier>1</AggressionInNeutralRegionDuringColdWarMultiplier>
          <OtherEmpireUnitsKilledDuringWarMultiplier>1</OtherEmpireUnitsKilledDuringWarMultiplier>
          <OtherEmpireCityBesiegedDuringWarMultiplier>1</OtherEmpireCityBesiegedDuringWarMultiplier>
          <OtherEmpireCitiesTakenDuringWarMultiplier>1</OtherEmpireCitiesTakenDuringWarMultiplier>
          <VillageConvertedMultiplierParameterName>1</VillageConvertedMultiplierParameterName>
          <WarMultiplier>0.5</WarMultiplier>
        </WarTermAgent>

        <TruceTermAgent>
          <WarFatigueMultiplier>1</WarFatigueMultiplier>
        </TruceTermAgent>

        <ColdWarTermAgent>
          <LivingspaceTenseMultiplier>1</LivingspaceTenseMultiplier>
          <UnitsInTerritoryMultiplier>1</UnitsInTerritoryMultiplier>
        </ColdWarTermAgent>

        <PeaceTermAgent>
          <PeacefulMultiplier>1</PeacefulMultiplier>
          <CommonFriendAllyMultiplier>1</CommonFriendAllyMultiplier>
          <CommonEnemyMultiplier>1</CommonEnemyMultiplier>
          <LastWarMultiplier>1</LastWarMultiplier>
          <CommonFrontierMultiplier>1</CommonFrontierMultiplier>
          <PositiveNegativeContractMultiplier>1</PositiveNegativeContractMultiplier>
        </PeaceTermAgent>

        <AllianceTermAgent>
          <PeacefulMultiplier>1</PeacefulMultiplier>
          <CommonFriendAllyMultiplier>1</CommonFriendAllyMultiplier>
          <CommonEnemyMultiplier>1</CommonEnemyMultiplier>
          <LastWarMultiplier>1</LastWarMultiplier>
          <PositiveNegativeContractMultiplier>1</PositiveNegativeContractMultiplier>
          <GiftMultiplier>1</GiftMultiplier>
        </AllianceTermAgent>

        <MapExchangeTermAgent>
          <ExplorationRatioScoreMultiplier>1</ExplorationRatioScoreMultiplier>
        </MapExchangeTermAgent>

        <VisionAndMapExchangeTermAgent>
          <Multiplier>1</Multiplier>
        </VisionAndMapExchangeTermAgent>

        <CommercialAgreementTermAgent>
          <Multiplier>1</Multiplier>
        </CommercialAgreementTermAgent>

        <ResearchAgreementTermAgent>
          <Multiplier>1</Multiplier>
        </ResearchAgreementTermAgent>

        <OpenBordersTermAgent>
          <Multiplier>1</Multiplier>
        </OpenBordersTermAgent>

        <MarketBanRemovalTermAgent>
          <Multiplier>1</Multiplier>
        </MarketBanRemovalTermAgent>

        <MapEmbargoTermAgent>
          <Multiplier>1</Multiplier>
